{
  "name": "curlhelper",
  "version": "0.1.9",
  "description": "A powerful, curl-like HTTP client for JavaScript and TypeScript with fluent API and curl command parsing",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "curl",
    "http",
    "https",
    "request",
    "fetch",
    "api",
    "client",
    "rest",
    "typescript",
    "fluent-api",
    "chainable"
  ],
  "author": "Steven Rugg",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stevenrugg/curlhelper.git"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@vitest/coverage-v8": "^1.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5",
    "vitest": "^1.0.0"
  },
  "dependencies": {},
  "engines": {
    "node": ">=18.0.0"
  }
}
